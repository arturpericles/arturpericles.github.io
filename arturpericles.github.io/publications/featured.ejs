<div class="list grid quarto-listing-cols-3">
  <% for (const item of items) { %>
    <div class="g-col-1" <%- metadataAttrs(item) %>>
      <a class="quarto-grid-link" href="<%- item.path %>">
        <div class="quarto-grid-item card h-100 card-left">
          <div class="card-body post-contents">
          
          <% if (item.title) { %>
            <h5 class="no-anchor card-title listing-title"><%- item.title %></h5>
          <% } %>

          <% if ("item.citation.container-title") { %>
            <div ><%= item.citation['container-title'] %></div>
            <% } %> 
        <div>
        <div class="card-attribution card-text-small">
          <% if (item.date) { %>
            <div class="listing-date"><%- item.date %></div>
          <% } %>
        </div>
        <div class="card-text-small card-other-values">
          <% if (item.citation.status) { %>
        
            <%- item.citation.status %>
        
          <% } %>

          <!-- <% if (item.citation.type) { %>
        
              <%- item.citation.type %>
        
          <% } %> -->
        </div>
      </div>
      </div>  

    </div>
      </a>
